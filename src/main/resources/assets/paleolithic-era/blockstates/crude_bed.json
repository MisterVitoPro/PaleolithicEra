{
  "multipart": [
    { "when": { "part": "foot", "facing": "north" }, "apply": { "model": "paleolithic-era:block/crude_bed_foot"} },
    { "when": { "part": "foot", "facing": "east"  }, "apply": { "model": "paleolithic-era:block/crude_bed_foot", "y": 90 } },
    { "when": { "part": "foot", "facing": "south" }, "apply": { "model": "paleolithic-era:block/crude_bed_foot", "y": 180 } },
    { "when": { "part": "foot", "facing": "west"  }, "apply": { "model": "paleolithic-era:block/crude_bed_foot", "y": 270 } },

    { "when": { "part": "head", "facing": "north" }, "apply": { "model": "paleolithic-era:block/crude_bed_head" } },
    { "when": { "part": "head", "facing": "east"  }, "apply": { "model": "paleolithic-era:block/crude_bed_head", "y": 90 } },
    { "when": { "part": "head", "facing": "south" }, "apply": { "model": "paleolithic-era:block/crude_bed_head", "y": 180 } },
    { "when": { "part": "head", "facing": "west"  }, "apply": { "model": "paleolithic-era:block/crude_bed_head", "y": 270 } }
  ]
}
